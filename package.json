{
  "name": "sugarshin-bolt",
  "version": "0.0.0",
  "description": "My Bolt",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "npm run lint -- --max-warnings 0",
    "clean:dist": "rm -rf ./dist",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/app.ts",
    "start": "node dist/app.js",
    "heroku-postbuild": "NODE_ENV=production npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "engines": {
    "node": "12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugarshin/sugarshin-bolt.git"
  },
  "author": "Shingo Sato",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sugarshin/sugarshin-bolt/issues"
  },
  "homepage": "https://github.com/sugarshin/sugarshin-bolt#readme",
  "dependencies": {
    "@octokit/rest": "16.43.2",
    "@slack/bolt": "1.8.1",
    "@slack/logger": "2.0.0",
    "cheerio": "1.0.0-rc.5",
    "cron": "1.8.2",
    "dotenv": "8.2.0",
    "got": "10.7.0",
    "iconv-lite": "0.6.2",
    "lodash": "4.17.20",
    "request": "2.88.2",
    "request-promise-native": "1.0.9"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.21",
    "@types/cron": "1.7.2",
    "@types/lodash": "4.14.161",
    "@types/request-promise-native": "1.0.17",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.3.0",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "ts-node-dev": "1.0.0-pre.62",
    "type-fest": "0.16.0",
    "typescript": "3.9.7"
  }
}
